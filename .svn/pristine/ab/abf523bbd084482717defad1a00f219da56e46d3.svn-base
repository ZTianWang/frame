define(function(require, exports) {
	require('pager');/*使用分页*/
	require('dPicker');
	/*得到本菜单对应的按钮额级function*/
 var  accountFunctions=roleFunctions['account/list'];
 /*根据权限，得到用户拥有的列表中的按钮，返回值是用户实际拥有的，参数是列表中的全部按钮*/
 var  listFunction=KU.getFunction(accountFunctions,['edit','audit']);
	function query() {/* 定义查询方法*/
		KU.send({
			url : 'account/page',
			eText : true,/* 根据状态码自动提示错误信息 */
			data : {
				account : $('#account').val(),
				cellphone : $('#cellphone').val(),
				email : $('#email').val(),
				startDate : $('#startDate').sd(),
				endDate : $('#endDate').ed(),
			},
			success : function(data) {
				data.functionBtns=listFunction;//指定列表模板中需要显示的按钮
				$('#resultTable').html(template('listTpl', data));/* 填充页面数据 */
				/*真实数据产生之后，针对按钮绑定事件*/
				bindFunction();
				/*进行分页操作*/
				data.query = query;
				$('#pager').pager(data);
			}
		});
	}
	/*判断列表按钮*/
function bindFunction(){
	    /*绑定所有funBtn为accountFunBtn的按钮click事件*/
		 $('input[funBtn=accountFunBtn]').on('click', function() {
			 /*根据按钮上的key的值，获得隐藏域对象*/
			 var $hiddenInfo=$('#'+$(this).attr('key'));
			 var flag=$(this).attr('flag');
			 /*接来下可以根据具体信息做任意处理*/
			 if(flag=='edit'){
				 alert('是编辑按钮');
			 }else if(flag=='audit'){
				 alert('是审核按钮');
			 }
			 var msg='点击了['+$(this).val()+']按钮,标示为['+$(this).attr('flag')+']';
			     msg+='\n账号是:'+$hiddenInfo.attr('account');
			     msg+='\n邮箱是:'+$hiddenInfo.attr('email');
			     msg+='\n手机号是:'+$hiddenInfo.attr('cellphone');
			     alert(msg);
			});
	}

exports.init = function() {
	    /*判断是用拥有的菜单中否拥有新增按钮*/
	var fun=KU.getFunction(accountFunctions,'add');
	if(fun){
		$('#addBtnHtml').html('<input type="button"    onclick="showHtml(\'account/add\');" name="'+fun.flag+'"  value="'+fun.name+'" />');
	 }
	/*新增按钮判断结束，其余依次类推*/
		query();/*默认直接查询*/
		$('#searchBtn').on('click', function() {
			query();
		});
	};


});