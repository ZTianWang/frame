define(function(require, exports, module) {
 	require.async('js/jquery/jquery.history.min',function(){
		$.History.bind(function(state) {
			if (state) {
				var q = state.indexOf('?');
				if (q != -1) {
					state = state.substring(0, q);
				}
				$('#mainHTML').load('/' + state + '.html');
			}
		});
	});
	require('dialog');
	$('#clickMe').on('click', function() {
		var d = dialog({
			title : '弹出组件',
			content : '2秒后消失'
		});
		d.show();
		setTimeout(function() {
			d.remove();
		}, 2000);
	}); 
});