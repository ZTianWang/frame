div, a, span, strong, i, td, th, em { font-size: 12px; }
.cursor { cursor: pointer; }
th.border-t{border-top:1px solid #e3e3e3 !important;}
.mb20{margin-bottom:20px !important;}
.border-b1{border-bottom:1px solid #e3e3e3 !important;}
.text-l{text-align:left !important;}
/*--页面结构--*/
.header { width: 100%; height: 50px; position: fixed; top: 0px; left: 0px; background: #324858; line-height: 50px; z-index: 1; }
.sidebar { width: 220px; position: fixed; top: 50px; left: 0px; bottom: 0px; background: #eeeef1; z-index: 2; border-right: 1px solid #eee; }
.content { position: absolute; top: 50px; left: 220px; right: 0px; bottom: 0px; padding-top: 41px; overflow-y: auto; }
.footer { margin: 5px 20px 0 20px; line-height: 50px; border-top: 1px solid #ddd; position: relative; }
.dialog-body { height: auto; }
/*--header--*/
.logo { width: 221px; height: 50px; background: #203545 url('../img/logo.png') no-repeat 70px -14px; }
.nav-bar { margin-bottom: 0px; }
.nav-bar .nav>li>a { padding: 15px; }
.nav-bar .signout { color: #ff6161; border-left: 1px solid #243d4e; padding-left: 20px; margin-right: 20px; }

/*--sidebar--*/
.menu li a { font-size: 14px; color: #999; padding: 14px 0 14px 20px; display: block; box-shadow: 0 1px 0 0 rgba(255, 255, 255, 0.4) inset; transition: color 0.5s ease 0s; }
.menu>li { border-bottom: 1px solid #e1e1e1; position: relative; }
.menu>li>a .fr-icon { margin-right: 15px; }
