/**页面组件样式   开始**/
html, body { width: 100%; height: 100%; font-family: 'Hiragino Sans GB', 'Microsoft YaHei', '宋体', Helvetica, Arial, sans-serif; font-size: 12px; color: #666; }
.main-body { overflow: hidden; }
