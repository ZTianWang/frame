/*--table 样式--*/
.table th { padding: 2px 15px; }
.table thead th { vertical-align: middle; }
.table tbody td i{background-image: url("../img/glyphicons-halflings-white.png");}
.table-footer td { padding: 18px 15px 20px 15px; background: #fafafa; }
.table-list th { text-align: left; background: #f6f6f6; border-top: 1px solid #e3e3e3 !important; border-bottom: 1px solid #e3e3e3; height: 35px; font-weight: normal; padding-left: 15px; box-shadow: 0px 1px 0px 0 #fff inset; }
.table-list td { padding: 15px; border-bottom: 1px solid #e3e3e3; }
.container-table tr td { padding: 5px 0px; }
.container-table tr td .area { display: inline-block; width: 50px; text-align: right; margin-right: 10px; }
.container-table tr td .area_top_40 { display: inline-block; width: 100px; text-align: right; margin-right: 10px; margin-top: 40px;}
.table-style thead th { padding: 7px 15px; }
.table-style tbody td, .table-style tbody th { padding: 5px 15px; }
.table-style1 { border-left: 0px solid #E8E8E8; border-bottom: 1px solid #E8E8E8; }
.table-style2, .table-style12 { margin-bottom: 0px; }
.table-style4 { border: 1px solid #E8E8E8; }
.table-style5 tr td { vertical-align: top; }
.table-style6 tbody+tbody { border-top: none; }
.table-bordered { border-radius: 0px; border-collapse: collapse; }
.table-style7 thead th:last-of-type { border-right: none; }
.table-style10 tr td:last-of-type, .table-style10 tbody tr th:last-of-type, .table-style12 tr td:last-of-type { /*border-right: none*/ }
.table-style10, .table-style12 { border-bottom: 1px solid #e3e3e3; }
.table-style11 tbody td:nth-child(7) span { width: 10px; white-space: nowrap; text-overflow: ellipsis; overflow: hidden; display: block; }
.table-style12 { border-top: none; border-bottom: 1px solid #E8E8E8; }
.table-style13 { border-top: 1px solid #eaeaea; border-left: 1px solid #eaeaea; }
.table-style13 th, .table-style13 td { padding: 5px 20px; border-right: 1px solid #eaeaea; border-bottom: 1px solid #eaeaea; text-align: center; }
.table-style13 th { background: #F2F2F2; }

/*--模拟的select--*/
.drop-down .dropdown-menu>li>a { padding: 5px 15px; }
.drop-down .dropdown-menu>li>a i { margin-right: 5px; }
.drop-down .dropdown-menu>li>a:hover { background: #e5ebf0; color: #324858; }
.select-container, .select-container1 { height: 35px; display: inline-block; width: 212px; position: relative; }
.select-container2{width:313px;}
.select-choice { border: 1px solid #aaa; height: 33px; line-height: 33px; padding: 0 0 0 8px; color: #444; position: relative; width: 100%; display: inline-block; cursor: pointer; }

.select-choice span { display: inline-block; overflow: hidden; width: 131px; text-overflow: ellipsis; white-space: nowrap; }
.select-choice div { position: absolute; top: 0px; right: 0px; height: 100%; border-left: 1px solid #aaa; width: 18px; background: #ccc; text-align: center; }
.select-choice div b { border-width: 0px 3px 6px; border-style: solid; border-color: transparent transparent  #888; display: inline-block; width: 0px; height: 0px; overflow: hidden; margin-top: 13px; }
.select-drop { position: relative; top: -5px; left: 0px; width: 200px; border: 1px solid #aaa; z-index: 100; height: 150px; overflow-y: auto; background: #fff; padding: 10px; }
.select-container2 .select-drop{width:300px;}
.select-container2 .select-choice span{width:271px;}

.select-drop1 { position: absolute; z-index: 1; top: 34px; }
.select-drop .icon { position: absolute; top: 10px; right: 10px; cursor: pointer; font-size: 16px; }
.select-dropdown-open { box-shadow: 0 1px 0 #fff inset; box-shadow: 0 1px 0 #fff inset; background-color: #eee; background-image: -webkit-gradient(linear, left bottom, left top, color-stop(0, white), color-stop(0.5, #eeeeee)); background-image: -webkit-linear-gradient(center bottom, white 0%, #eeeeee 50%); background-image: -moz-linear-gradient(center bottom, white 0%, #eeeeee 50%); background-image: -o-linear-gradient(bottom, white 0%, #eeeeee 50%); background-image: -ms-linear-gradient(top, #ffffff 0%, #eeeeee 50%); background-image: linear-gradient(top, #ffffff 0%, #eeeeee 50%); }
.select-dropdown-open .select-choice div b { border-width: 6px 3px 0px; border-style: solid; border-color: #888 transparent transparent; }
.select-100{width:100px;}
.select-120{width:120px;}
.select-250{width:250px;}
.select-300{width:300px;}
/*--公共样式--*/
a { cursor: pointer; text-decoration: none; outline: none; }
a:hover, a:visited, a:focus { text-decoration: none; outline: none; }
li { list-style: none; }
ul, ol { margin: 0px; }
em { font-style: normal; }
.no-radius { border-radius: none; }
.hide1 { opacity: 0; filter: alpha(opacity=0); position: absolute; }
.hide2 { opacity: 0; filter: alpha(opacity=0); }
.hide { display: none; }
.lh25 { line-height: 22px; }

/*--外边局--*/
.ml20 { margin-left: 20px !important; }
.mt20 { margin-top: 20px; }
.ml { margin-left: 10px; }
.mr { margin-right: 10px; }
.mb { margin-bottom: 10px; }
.mt5 { margin-top: 5px; }
.mr5 { margin-right: 5px; }
.ml5 { margin-left: 5px; }
.ml50 { margin-left: 50px !important; }
.m20 { margin: 20px; }
.mr50 { margin-right: 50px; }
.ver { vertical-align: -2px; }
.bold { font-weight: bold; }
.w { width: 100%; }
.mb0 { margin-bottom: 0px !important; }
.md{vertical-align:middle;}
.dib{display:inline-block;}
.md2{vertical-align:-2px;}

/*--内边距-*/
.pt { padding-top: 10px; }
.pt { padding-bottom: 10px; }

/*--字体颜色--*/
.fs7 { color: #fff; }
.fs3 { color: #F00; }
.fs4 { color: #65933d; }
.fs1 { color: #ff7200 !important; }
.fs2 { color: #08c; }

/*--字体大小--*/
.sz12 { font-size: 12px !important; }
.sz14 { font-size: 14px; }
.sz16 { font-size: 16px !important; }
.sz18 { font-size: 18px !important; }
.sz20 { font-size: 20px !important; }

/*--按钮--*/
.btn-primary { background-image: none; }

/*--border--*/
.bt { border-top: 1px solid #E8E8E8; }
.bm, .bb { border-bottom: 1px solid #E8E8E8; }
.del-b { border: none !important }
.del-bt { border-top: none !important }
.del-br { border-right: none !important }
.del-bb { border-bottom: none; }
.del-bl { border-left: none !important }
/*--单选--*/
.radio-box .radio { padding: 0px !important; border: 2px solid #EAEAEA; line-height: 32px; border-radius: 3px; margin-right: 10px; width: 90px; line-height: 32px; text-align: center; display: inline-block; }
.radio-box .radio.active { border-color: #7496B0; }

/*--多选--*/
.checkbox-box { margin-top: 3px; }
.checkbox-box span { margin-right: 40px; }
.checkbox-box span .ver { margin-left: 3px; }

/*--textarea--*/
textarea { resize: none; }
.textarea-box, .textarea-box1 { border: 1px solid #ccc; overflow: hidden; border-radius: 3px; }
.textarea-box1, .textarea2 { width: 80%; }
.textarea1 { width: 100%; border: none; height: 100%; margin-bottom: 0px; }
.textarea { border: 1px solid #ccc; resize: none; width: 100%; }

/*--文本位置--*/
.align-center { text-align: center !important; }

/*--remove float--*/
.cl:after { content: "."; display: block; height: 0; clear: both; visibility: hidden; }
.cl { zoom: 1; }
/*内容区域tubiao统一显示的位置*/
