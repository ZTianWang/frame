<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
   
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    <typeAliases>
    <!--指定自动扫描entity中的类，默认的名字为： javabean 的首字母小写的非限定类名来作为它的别名
    例如com.frame.entity.User   可以在UserMapper.xml中的resultType中指定为user
    -->
    <package name="com.frame.entity"/>
    </typeAliases>
</configuration>